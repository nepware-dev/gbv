
{%
  set classes = [
    'block',
    'vacancies-block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
    'col-sm-8 offset-md-2',
  ]
%}
<div{{ attributes.addClass(classes) }} id="vacancies" data-title="{{label}}">
  {{ title_prefix }}
  {% if label %}
    <h2{{ title_attributes }} class="underline-h2-tag">{{ label }}</h2>
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    <div{{ content_attributes.addClass('content') }}>
    <div class="container">
     {{ content }}
     </div>
    </div>
  {% endblock %}
</div>

