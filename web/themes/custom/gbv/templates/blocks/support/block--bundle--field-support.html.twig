{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
    'field-support',
    'col-sm-8 offset-md-2',
  ]
%}
{%
    set contentClasses = [
        'content'
    ]
%}
<div{{ attributes.addClass(classes) }} id="field-support" data-title="{{ label }}">
  <div class="container">
    <div class="row support">
        {{ title_prefix }}
        {% if label %}
          <h2{{ title_attributes }}>{{ label }}</h2>
        {% endif %}
        {{ title_suffix }}
        {% block content %}
          {{content.body}}
        {% endblock %}
    </div>
  </div>
</div>


