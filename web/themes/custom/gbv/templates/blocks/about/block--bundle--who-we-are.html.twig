{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
    'who-are-we',
    'container',
    'col-12 col-md-8 offset-md-2',
  ]
%}
{%
    set contentClasses = [
        'content',
    ]
%}
<div{{ attributes.addClass(classes) }} id="who-we-are" data-title="{{ label }}">
  {{ title_prefix }}
  {% if label %}
    <h2{{ title_attributes }} class="underline-h2-tag">{{ label }}</h2>
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    <div{{ content_attributes.addClass(contentClasses) }}>
      {{ content }}
    </div>
  {% endblock %}
</div>