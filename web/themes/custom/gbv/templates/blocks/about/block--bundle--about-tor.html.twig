{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
  ]
%}
{%
    set contentClasses = [
        'content'
    ]
%}
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% if label %}
    {# <h2{{ title_attributes }}>{{ label }}</h2> #}
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    <div{{ content_attributes.addClass(contentClasses) }}>
        <div class="about-tor-section">
            <div class="container">
                <div class="row">
                    <div class="col-3 tor-sidebar">
                        {{content.field_tor_sidebar}}
                    </div>
                    <div class="col-9 tor-content">
                        {{content.field_tor_body}}
                    </div>
                </div>
            </div>
        </div>
    </div>
  {% endblock %}
</div>


