{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
    'col-12 col-md-8 offset-md-2',
  ]
%}
{%
    set contentClasses = [
        'content'
    ]
%}
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% if label %}
    {# <h2{{ title_attributes }}>{{ label }}</h2> #}
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    <div{{ content_attributes.addClass(contentClasses) }}>
        <div class="about-tor-section">
            <div class="container">
                  <div class="tor-content">
                      {{content.field_tor_body}}
                  </div>
            </div>
        </div>
    </div>
  {% endblock %}
</div>


