{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
    'global-architecture-section',
  ]
%}
{%
    set contentClasses = [
        'content'
    ]
%}
<div{{ attributes.addClass(classes) }} id="gpc-architecture" data-title="GPC ARCHITECTURE">
  <div class="col-md-8 offset-md-1 gpc-architect">
    {{ title_prefix }}
    {% if label %}
      <h2{{ title_attributes }}>{{ label }}</h2>
    {% endif %}
    {{ title_suffix }}
    {% block content %}
      <div{{ content_attributes.addClass(contentClasses) }}>
          <div class="container">
              {{content.body}}
          </div>
          <img src="{{directory}}/images/gpca.png">
      </div>
    {% endblock %}
  </div>
</div>


