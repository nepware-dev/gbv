{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/addtoany": "^1.15",
        "drupal/anchor_link": "^2.5",
        "drupal/better_exposed_filters": "^3.0",
        "drupal/bootstrap_barrio": "^4.22",
        "drupal/ckeditor_balloonpanel": "^1.1",
        "drupal/ckeditor_font": "^1.2",
        "drupal/ckeditor_iframe": "^2.1",
        "drupal/ckeditor_inserthtml": "^1.0",
        "drupal/ckeditor_smiley": "^1.0",
        "drupal/ckeditor_uploadimage": "^2.0",
        "drupal/ckeditor_videodetector": "^1.0",
        "drupal/ckeditor_youtube": "^1.3",
        "drupal/color_field": "^2.4",
        "drupal/colorbutton": "^1.3",
        "drupal/conditional_fields": "^1.0",
        "drupal/console": "^1.0.2",
        "drupal/contact_storage": "^1.0",
        "drupal/core": "^8.6.0",
        "drupal/core-composer-scaffold": "^9.4",
        "drupal/country": "^1.0@RC",
        "drupal/ctools": "^3.2",
        "drupal/devel": "^2.0",
        "drupal/ds": "^3.3",
        "drupal/facets": "^1.4",
        "drupal/flags": "^1.0@beta",
        "drupal/form_placeholder": "^1.0",
        "drupal/google_analytics": "^4.0",
        "drupal/google_analytics_reports": "^3.0",
        "drupal/honeypot": "^2.0",
        "drupal/image_resize_filter": "^1.1",
        "drupal/inline_responsive_images": "^2.2",
        "drupal/mailchimp": "^1.8",
        "drupal/page_manager": "^4.0",
        "drupal/panels": "^4.4",
        "drupal/pathauto": "^1.4",
        "drupal/search_api": "^1.13",
        "drupal/smtp": "^1.0",
        "drupal/social_media_links": "^2.6",
        "drupal/token": "^1.5",
        "drupal/twig_tweak": "^2.3",
        "drupal/video": "^1.4",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "^2.6",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "drupal/coder": "^8.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "composer/installers": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/console-extend-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "cghooks": "vendor/bin/cghooks",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion",
            "cghooks update"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "cghooks add --ignore-lock"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "hooks": {
            "pre-commit": [
                "echo pre",
                "./hooks/js-lint.sh",
                "./hooks/php-lint.sh"
            ]
        }
    }
}
